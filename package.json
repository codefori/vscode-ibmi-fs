{
  "name": "vscode-ibmi-fs",
  "displayName": "IBM i FileSystem",
  "description": "File system provider for IBM i QSYS",
  "version": "0.0.3-dev0",
  "preview": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "author": {
    "name": "Code4i",
    "url": "https://github.com/codefori"
  },
  "publisher": "halcyontechltd",
  "repository": {
    "url": "https://github.com/codefori/vscode-ibmi-fs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "HalcyonTechLtd.code-for-ibmi"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-ibmi-fs.editor",
        "displayName": "Object Editor",
        "selector": [
          {
            "filenamePattern": "object*/**/*.BNDDIR"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          },
          {
            "filenamePattern": "object*/**/*.DTAARA"
          },
          {
            "filenamePattern": "object*/**/*.{PGM,SRVPGM}"
          },
          {
            "filenamePattern": "object*/**/*.CMD"
          },
          {
            "filenamePattern": "object*/**/*.DTAQ"
          },
          {
            "filenamePattern": "object*/**/*.FILE"
          },
          {
            "filenamePattern": "object*/**/*.JOBQ"
          },
          {
            "filenamePattern": "object*/**/*.JOBD"
          },
          {
            "filenamePattern": "object*/**/*.OUTQ"
          },
          {
            "filenamePattern": "object*/**/*.USRSPC"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          },
          {
            "filenamePattern": "object*/**/*.MODULE"
          },
          {
            "filenamePattern": "object*/**/*.JRNRCV"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "vscode-ibmi-fs.downloadSavf",
        "title": "Download Save File",
        "category": "IBM i FS",
        "icon": "$(cloud-download)"
      },
      {
        "command": "vscode-ibmi-fs.uploadSavf",
        "title": "Upload Save File",
        "category": "IBM i FS",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "vscode-ibmi-fs.clearSavf",
        "title": "Clear Save File",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.savf",
        "title": "Save to Save File",
        "category": "IBM i FS",
        "icon": "$(save)"
      },
      {
        "command": "vscode-ibmi-fs.restore",
        "title": "Restore from Save File",
        "category": "IBM i FS",
        "icon": "$(issue-reopened)"
      },
      {
        "command": "vscode-ibmi-fs.clearDataQueue",
        "title": "Clear Data Queue",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.sendToDataQueue",
        "title": "Send data to Data Queue",
        "category": "IBM i FS",
        "icon": "$(arrow-right)"
      },
      {
        "command": "vscode-ibmi-fs.HldJobq",
        "title": "Hold Job Queue",
        "category": "IBM i FS",
        "icon": "$(primitive-square)"
      },
      {
        "command": "vscode-ibmi-fs.RlsJobq",
        "title": "Release Job Queue",
        "category": "IBM i FS",
        "icon": "$(play)"
      },
      {
        "command": "vscode-ibmi-fs.ClrJobq",
        "title": "Clear Job Queue",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.HldJob",
        "title": "Hold job",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.RlsJob",
        "title": "Release Job",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.EndJob",
        "title": "End Job",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.HldOutq",
        "title": "Hold Output Queue",
        "category": "IBM i FS",
        "icon": "$(primitive-square)"
      },
      {
        "command": "vscode-ibmi-fs.RlsOutq",
        "title": "Release Output Queue",
        "category": "IBM i FS",
        "icon": "$(play)"
      },
      {
        "command": "vscode-ibmi-fs.ClrOutq",
        "title": "Clear Output Queue",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.MngWtr",
        "title": "Manage Writer",
        "category": "IBM i FS",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "vscode-ibmi-fs.EndWtr",
        "title": "End Writer",
        "category": "IBM i FS",
        "icon": "debug-stop"
      },
      {
        "command": "vscode-ibmi-fs.GenPdf",
        "title": "Generate PDF",
        "category": "IBM i FS",
        "icon": "file-pdf"
      },
      {
        "command": "vscode-ibmi-fs.DelSpool",
        "title": "Delete spool",
        "category": "IBM i FS",
        "icon": "trash"
      },
      {
        "command": "vscode-ibmi-fs.DelOldSpool",
        "title": "Delete old spools",
        "category": "IBM i FS",
        "icon": "$(calendar)"
      },
      {
        "command": "vscode-ibmi-fs.ChgDtaara",
        "title": "Change DTAARA",
        "category": "IBM i FS",
        "icon": "$(edit)"
      },
      {
        "command": "vscode-ibmi-fs.chgUsrspc",
        "title": "Change USRSPC",
        "category": "IBM i FS",
        "icon": "$(edit)"
      },
      {
        "command": "vscode-ibmi-fs.Addbnddire",
        "title": "Add Binding Directory Entry",
        "category": "IBM i FS",
        "icon": "$(plus)"
      },
      {
        "command": "vscode-ibmi-fs.Rmvbnddire",
        "title": "Remove Binding Directory Entry",
        "category": "IBM i FS",
        "icon": "trash"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "vscode-ibmi-fs.sendToDataQueue",
          "when": "resourceExtname == .DTAQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.clearDataQueue",
          "when": "resourceExtname == .DTAQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.downloadSavf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.uploadSavf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.savf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@3"
        },
        {
          "command": "vscode-ibmi-fs.restore",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@4"
        },
        {
          "command": "vscode-ibmi-fs.clearSavf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@5"
        },
        {
          "command": "vscode-ibmi-fs.HldJobq",
          "when": "resourceExtname == .JOBQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.RlsJobq",
          "when": "resourceExtname == .JOBQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.ClrJobq",
          "when": "resourceExtname == .JOBQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@3"
        },
        {
          "command": "vscode-ibmi-fs.HldOutq",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.RlsOutq",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.ClrOutq",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@3"
        },
        {
          "command": "vscode-ibmi-fs.ChgDtaara",
          "when": "resourceExtname == .DTAARA && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.DelOldSpool",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@4"
        },
        {
          "command": "vscode-ibmi-fs.MngWtr",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@5"
        },
        {
          "command": "vscode-ibmi-fs.chgUsrspc",
          "when": "resourceExtname == .USRSPC && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.Addbnddire",
          "when": "resourceExtname == .BNDDIR && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
      ],
      "commandPalette": [
        {
          "command": "vscode-ibmi-fs.downloadSavf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.uploadSavf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.clearSavf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.savf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.restore",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.clearDataQueue",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.sendToDataQueue",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.HldJobq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.RlsJobq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.ClrJobq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.HldJob",
          "when": "never"
        },
                {
          "command": "vscode-ibmi-fs.RlsJob",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.EndJob",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.HldOutq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.RlsOutq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.MngWtr",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.EndWtr",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.ClrOutq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.GenPdf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.DelSpool",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.DelOldSpool",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.ChgDtaara",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.chgUsrspc",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.Addbnddire",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.Rmvbnddire",
          "when": "never"
        }
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "package": "npx @vscode/vsce package --pre-release",
    "vscode:prepublish": "npm run build",
    "compile": "npm run clean && webpack",
    "watch": "npm run clean && webpack --watch",
    "build": "npm run clean && webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@halcyontech/vscode-ibmi-types": "^2.17.2",
    "@types/node": "^20",
    "@types/vscode": "^1.90.0",
    "esbuild": "^0.19.12",
    "esbuild-loader": "^4.2.2",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@microsoft/fast-element": "^2.9.1",
    "@vscode/webview-ui-toolkit": "^1.4.0"
  }
}
