{
  "name": "vscode-ibmi-fs",
  "displayName": "%displayName%",
  "description": "%description%",
  "l10n": "./l10n",
  "version": "0.0.4-dev0",
  "preview": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "author": {
    "name": "Code4i",
    "url": "https://github.com/codefori"
  },
  "publisher": "halcyontechltd",
  "repository": {
    "url": "https://github.com/codefori/vscode-ibmi-fs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "HalcyonTechLtd.code-for-ibmi"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-ibmi-fs.editor",
        "displayName": "Object Editor",
        "selector": [
          {
            "filenamePattern": "object*/**/*.BNDDIR"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          },
          {
            "filenamePattern": "object*/**/*.DTAARA"
          },
          {
            "filenamePattern": "object*/**/*.{PGM,SRVPGM}"
          },
          {
            "filenamePattern": "object*/**/*.CMD"
          },
          {
            "filenamePattern": "object*/**/*.DTAQ"
          },
          {
            "filenamePattern": "object*/**/*.FILE"
          },
          {
            "filenamePattern": "object*/**/*.JOBQ"
          },
          {
            "filenamePattern": "object*/**/*.JOBD"
          },
          {
            "filenamePattern": "object*/**/*.OUTQ"
          },
          {
            "filenamePattern": "object*/**/*.USRSPC"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          },
          {
            "filenamePattern": "object*/**/*.MODULE"
          },
          {
            "filenamePattern": "object*/**/*.JRNRCV"
          },
          {
            "filenamePattern": "object*/**/*.JRN"
          },
          {
            "filenamePattern": "object*/**/*.SBSD"
          },
          {
            "filenamePattern": "object*/**/*.CLS"
          },
          {
            "filenamePattern": "object*/**/*.QRYDFN"
          },
          {
            "filenamePattern": "object*/**/*.MSGQ"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "vscode-ibmi-fs.downloadSavf",
        "title": "%command.downloadSavf.title%",
        "category": "IBM i FS",
        "icon": "$(cloud-download)"
      },
      {
        "command": "vscode-ibmi-fs.uploadSavf",
        "title": "%command.uploadSavf.title%",
        "category": "IBM i FS",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "vscode-ibmi-fs.clearSavf",
        "title": "%command.clearSavf.title%",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.savf",
        "title": "%command.savf.title%",
        "category": "IBM i FS",
        "icon": "$(save)"
      },
      {
        "command": "vscode-ibmi-fs.restore",
        "title": "%command.restore.title%",
        "category": "IBM i FS",
        "icon": "$(issue-reopened)"
      },
      {
        "command": "vscode-ibmi-fs.clearDataQueue",
        "title": "%command.clearDataQueue.title%",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.sendToDataQueue",
        "title": "%command.sendToDataQueue.title%",
        "category": "IBM i FS",
        "icon": "$(arrow-right)"
      },
      {
        "command": "vscode-ibmi-fs.HldJobq",
        "title": "%command.HldJobq.title%",
        "category": "IBM i FS",
        "icon": "$(primitive-square)"
      },
      {
        "command": "vscode-ibmi-fs.RlsJobq",
        "title": "%command.RlsJobq.title%",
        "category": "IBM i FS",
        "icon": "$(play)"
      },
      {
        "command": "vscode-ibmi-fs.ClrJobq",
        "title": "%command.ClrJobq.title%",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.HldOutq",
        "title": "%command.HldOutq.title%",
        "category": "IBM i FS",
        "icon": "$(primitive-square)"
      },
      {
        "command": "vscode-ibmi-fs.RlsOutq",
        "title": "%command.RlsOutq.title%",
        "category": "IBM i FS",
        "icon": "$(play)"
      },
      {
        "command": "vscode-ibmi-fs.ClrOutq",
        "title": "%command.ClrOutq.title%",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.MngWtr",
        "title": "%command.MngWtr.title%",
        "category": "IBM i FS",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "vscode-ibmi-fs.DelOldSpool",
        "title": "%command.DelOldSpool.title%",
        "category": "IBM i FS",
        "icon": "$(calendar)"
      },
      {
        "command": "vscode-ibmi-fs.ChgDtaara",
        "title": "%command.ChgDtaara.title%",
        "category": "IBM i FS",
        "icon": "$(edit)"
      },
      {
        "command": "vscode-ibmi-fs.chgUsrspc",
        "title": "%command.chgUsrspc.title%",
        "category": "IBM i FS",
        "icon": "$(edit)"
      },
      {
        "command": "vscode-ibmi-fs.Addbnddire",
        "title": "%command.Addbnddire.title%",
        "category": "IBM i FS",
        "icon": "$(plus)"
      },
      {
        "command": "vscode-ibmi-fs.GenJrnRcv",
        "title": "%command.GenJrnRcv.title%",
        "category": "IBM i FS",
        "icon": "$(git-pull-request-new-changes)"
      },
      {
        "command": "vscode-ibmi-fs.DspJrn",
        "title": "%command.DspJrn.title%",
        "category": "IBM i FS",
        "icon": "$(file-code)"
      },
      {
        "command": "vscode-ibmi-fs.StrSbs",
        "title": "%command.StrSbs.title%",
        "category": "IBM i FS",
        "icon": "$(play)"
      },
      {
        "command": "vscode-ibmi-fs.EndSbs",
        "title": "%command.EndSbs.title%",
        "category": "IBM i FS",
        "icon": "$(primitive-square)"
      },
      {
        "command": "vscode-ibmi-fs.clearMessageQueue",
        "title": "%command.clearMessageQueue.title%",
        "category": "IBM i FS",
        "icon": "$(trash)"
      },
      {
        "command": "vscode-ibmi-fs.QueryFile",
        "title": "%command.QueryFile.title%",
        "category": "IBM i FS",
        "icon": "$(file-code)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "vscode-ibmi-fs.sendToDataQueue",
          "when": "resourceExtname == .DTAQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.clearDataQueue",
          "when": "resourceExtname == .DTAQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.downloadSavf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor && ibmiFileType == 'SAVF'",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.uploadSavf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor && ibmiFileType == 'SAVF'",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.savf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor && ibmiFileType == 'SAVF'",
          "group": "navigation@3"
        },
        {
          "command": "vscode-ibmi-fs.restore",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor && ibmiFileType == 'SAVF'",
          "group": "navigation@4"
        },
        {
          "command": "vscode-ibmi-fs.clearSavf",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor && ibmiFileType == 'SAVF'",
          "group": "navigation@5"
        },
        {
          "command": "vscode-ibmi-fs.HldJobq",
          "when": "resourceExtname == .JOBQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.RlsJobq",
          "when": "resourceExtname == .JOBQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.ClrJobq",
          "when": "resourceExtname == .JOBQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@3"
        },
        {
          "command": "vscode-ibmi-fs.HldOutq",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.RlsOutq",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.ClrOutq",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@3"
        },
        {
          "command": "vscode-ibmi-fs.ChgDtaara",
          "when": "resourceExtname == .DTAARA && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.DelOldSpool",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@4"
        },
        {
          "command": "vscode-ibmi-fs.MngWtr",
          "when": "resourceExtname == .OUTQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@5"
        },
        {
          "command": "vscode-ibmi-fs.chgUsrspc",
          "when": "resourceExtname == .USRSPC && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.Addbnddire",
          "when": "resourceExtname == .BNDDIR && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.GenJrnRcv",
          "when": "resourceExtname == .JRN && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.DspJrn",
          "when": "resourceExtname == .JRN && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.StrSbs",
          "when": "resourceExtname == .SBSD && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.EndSbs",
          "when": "resourceExtname == .SBSD && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@2"
        },
        {
          "command": "vscode-ibmi-fs.clearMessageQueue",
          "when": "resourceExtname == .MSGQ && activeCustomEditorId == vscode-ibmi-fs.editor",
          "group": "navigation@1"
        },
        {
          "command": "vscode-ibmi-fs.QueryFile",
          "when": "resourceExtname == .FILE && activeCustomEditorId == vscode-ibmi-fs.editor && (ibmiFileType == 'PF' || ibmiFileType == 'LF')",
          "group": "navigation@6"
        }
      ],
      "view/item/context": [],
      "commandPalette": [
        {
          "command": "vscode-ibmi-fs.downloadSavf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.uploadSavf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.clearSavf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.savf",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.restore",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.clearDataQueue",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.sendToDataQueue",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.HldJobq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.RlsJobq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.ClrJobq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.HldOutq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.RlsOutq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.MngWtr",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.ClrOutq",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.DelOldSpool",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.ChgDtaara",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.chgUsrspc",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.Addbnddire",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.GenJrnRcv",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.DspJrn",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.StrSbs",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.EndSbs",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.clearMessageQueue",
          "when": "never"
        },
        {
          "command": "vscode-ibmi-fs.QueryFile",
          "when": "never"
        }
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "package": "npx @vscode/vsce package --pre-release",
    "vscode:prepublish": "npm run build",
    "compile": "npm run clean && webpack",
    "watch": "npm run clean && webpack --watch",
    "build": "npm run clean && webpack --mode production --devtool hidden-source-map",
    "nls": "npx @vscode/l10n-dev export --outDir ./l10n ./src",
    "pseudo-nls": "npx @vscode/l10n-dev generate-pseudo -o ./l10n/ ./l10n/bundle.l10n.json ./package.nls.json"
  },
  "devDependencies": {
    "@halcyontech/vscode-ibmi-types": "^2.17.2",
    "@types/node": "^20",
    "@types/vscode": "^1.90.0",
    "@vscode/l10n-dev": "^0.0.35",
    "esbuild": "^0.19.12",
    "esbuild-loader": "^4.2.2",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@microsoft/fast-element": "^2.10.0",
    "@vscode-elements/elements": "^2.5.1"
  }
}
