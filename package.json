{
  "name": "vscode-ibmi-fs",
  "displayName": "IBM i FileSystem",
  "description": "File system provider for IBM i QSYS",
  "version": "0.0.3-dev0",
  "preview": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.90.0"
  },
  "author": {
    "name": "Code4i",
    "url": "https://github.com/codefori"
  },
  "publisher": "halcyontechltd",
  "repository": {
    "url": "https://github.com/codefori/vscode-ibmi-fs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "HalcyonTechLtd.code-for-ibmi"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-ibmi-fs.editor",
        "displayName": "Object Editor",
        "selector": [
          {
            "filenamePattern": "object*/**/*.BNDDIR"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          },
          {
            "filenamePattern": "object*/**/*.DTAARA"
          },
          {
            "filenamePattern": "object*/**/*.{PGM,SRVPGM}"
          },
          {
            "filenamePattern": "object*/**/*.CMD"
          },
          {
            "filenamePattern": "object*/**/*.DTAQ"
          },
          {
            "filenamePattern": "object*/**/*.FILE"
          },
          {
            "filenamePattern": "object*/**/*.JOBQ"
          },
          {
            "filenamePattern": "object*/**/*.JOBD"
          },
          {
            "filenamePattern": "object*/**/*.OUTQ"
          },
          {
            "filenamePattern": "object*/**/*.USRSPC"
          },
          {
            "filenamePattern": "object*/**/*.MSGF"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "vscode-ibmi-fs.downloadSavf",
        "title": "Download Save file",
        "category": "IBM i FS",
        "icon": "cloud-download"
      },
      {
        "command": "vscode-ibmi-fs.uploadSavf",
        "title": "Upload Save file(s)",
        "category": "IBM i FS",
        "icon": "cloud-upload"
      },
      {
        "command": "vscode-ibmi-fs.clearSavf",
        "title": "Clear Save file",
        "category": "IBM i FS",
        "icon": "clear-all"
      },
      {
        "command": "vscode-ibmi-fs.savf",
        "title": "Save to Save file",
        "category": "IBM i FS",
        "icon": "save"
      },
      {
        "command": "vscode-ibmi-fs.restore",
        "title": "Restore from Save file",
        "category": "IBM i FS",
        "icon": "folder-opened"
      },
      {
        "command": "vscode-ibmi-fs.clearDataQueue",
        "title": "Clear",
        "category": "IBM i FS",
        "icon": "clear-all"
      },
      {
        "command": "vscode-ibmi-fs.sendToDataQueue",
        "title": "Send data",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.HldJobq",
        "title": "Hold Jobq",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.RlsJobq",
        "title": "Release Jobq",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.ClrJobq",
        "title": "Clear Jobq",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.HldJob",
        "title": "Hold job",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
            {
        "command": "vscode-ibmi-fs.RlsJob",
        "title": "Release Job",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
            {
        "command": "vscode-ibmi-fs.EndJob",
        "title": "End Job",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.HldOutq",
        "title": "Hold Outq",
        "category": "IBM i FS",
        "icon": "debug-pause"
      },
      {
        "command": "vscode-ibmi-fs.RlsOutq",
        "title": "Release Outq",
        "category": "IBM i FS",
        "icon": "debug-continue"
      },
      {
        "command": "vscode-ibmi-fs.ClrOutq",
        "title": "Clear Outq",
        "category": "IBM i FS",
        "icon": "clear-all"
      },
      {
        "command": "vscode-ibmi-fs.StrWtr",
        "title": "Start Writer",
        "category": "IBM i FS",
        "icon": "play"
      },
      {
        "command": "vscode-ibmi-fs.EndWtr",
        "title": "End Writer",
        "category": "IBM i FS",
        "icon": "debug-stop"
      },
      {
        "command": "vscode-ibmi-fs.GenPdf",
        "title": "Generate PDF",
        "category": "IBM i FS",
        "icon": "file-pdf"
      },
      {
        "command": "vscode-ibmi-fs.DelSpool",
        "title": "Delete spool",
        "category": "IBM i FS",
        "icon": "trash"
      },
      {
        "command": "vscode-ibmi-fs.DelOldSpool",
        "title": "Delete old spools",
        "category": "IBM i FS",
        "icon": "calendar"
      },
      {
        "command": "vscode-ibmi-fs.ChgDtaara",
        "title": "Change DTAARA",
        "category": "IBM i FS",
        "icon": "arrow-right"
      },
      {
        "command": "vscode-ibmi-fs.chgUsrspc",
        "title": "Change USRSPC",
        "category": "IBM i FS",
        "icon": "arrow-right"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "vscode-ibmi-fs.downloadSavf",
          "when": "view == objectBrowser && viewItem == object.file.savf",
          "group": "5_SavfAction@1"
        },
        {
          "command": "vscode-ibmi-fs.uploadSavf",
          "when": "view == objectBrowser && viewItem == filter",
          "group": "5_SavfAction@2"
        },
        {
          "command": "vscode-ibmi-fs.clearSavf",
          "when": "view == objectBrowser && viewItem == object.file.savf",
          "group": "5_SavfAction@3"
        },
        {
          "command": "vscode-ibmi-fs.savf",
          "when": "view == objectBrowser && viewItem == filter",
          "group": "5_SavfAction@4"
        },
        {
          "command": "vscode-ibmi-fs.restore",
          "when": "view == objectBrowser && viewItem == object.file.savf",
          "group": "5_SavfAction@5"
        },
        {
          "command": "vscode-ibmi-fs.sendToDataQueue",
          "when": "view == objectBrowser && viewItem == object.dtaq",
          "group": "5_DtaqAction@1"
        },
        {
          "command": "vscode-ibmi-fs.clearDataQueue",
